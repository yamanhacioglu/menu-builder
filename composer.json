{
    "name": "yamanhacioglu/menu-builder",
    "description": "Menu Builder For Laravel 11",
    "keywords": ["laravel", "menu", "menubuilder", "nestable", "vuejs3", "jquery", "draggable", "drag", "drop"],
    "homepage": "https://github.com/yamanhacioglu/menu-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaman HACIOÄžLU",
            "email": "yaman@northlab.com.tr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "YamanHacioglu\\MenuBuilder\\": "src/",
            "YamanHacioglu\\MenuBuilder\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YamanHacioglu\\MenuBuilder\\Tests\\": "tests/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "YamanHacioglu\\MenuBuilder\\MenuBuilderServiceProvider"
            ],
            "aliases": {
                "MenuBuilder": "YamanHacioglu\\MenuBuilder\\Facades\\MenuBuilder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
